REACT_APP_NETWORK_URL=""
REACT_APP_GOOGLE_ANALYTICS_ID=""
SKIP_PREFLIGHT_CHECK=true
REACT_APP_PUBLIC_URL="https://info.cheeseswap.app"
REACT_APP_NODE_ENV="production"

# The Graph Production API - Replace YOUR_API_KEY with your actual API key from https://thegraph.com/studio/apikeys/
